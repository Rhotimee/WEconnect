{
  "name": "weconnect",
  "version": "1.0.0",
  "description": "WeConnect provides a platform that brings businesses and individuals together.",
  "main": "./server/server.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "nodemon --exec babel-node server/server.js",
    "build": "babel server -d ./dist --ignore server/dist",
    "unmigrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all",
    "seed": "node ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "migrate": "npm run unmigrate && node ./node_modules/sequelize-cli/lib/sequelize db:migrate && npm run seed",
    "postinstall": "npm run build",
    "test": "npm run migrate && nyc --reporter=html --reporter=text mocha server/tests/ --exit   --timeout 7000 --compilers js:babel-register",
    "test-watch": "nodemon --exec \"npm test\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "jest": "jest",
    "e2e": "node_modules/.bin/nightwatch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rhotimee/weconnect.git"
  },
  "author": "Yemitan Isaiah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rhotimee/weconnect/issues"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "tests/e2e/"
    ],
    "verbose": true,
    "collectCoverage": true,
    "setupFiles": [
      "<rootDir>/client/tests/setup.js",
      "<rootDir>/client/tests/mock/localStorage.js"
    ],
    "roots": [
      "<rootDir>/client/tests"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__tests__/__mockData__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/client/tests/mock/styleMock.js"
    }
  },
  "dependencies": {
    "alertifyjs": "^1.11.1",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cloudinary": "^1.11.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "expect": "^23.4.0",
    "express": "~4.15.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^23.3.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.10",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "node-sass": "^4.8.3",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.1",
    "rc-pagination": "^1.16.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.1.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.4.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.7",
    "sequelize": "^5.15.1",
    "sequelize-cli": "^4.0.0",
    "style-loader": "^0.20.3",
    "swagger-ui-express": "^2.0.15",
    "url-loader": "^1.0.1",
    "validator": "^9.4.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.1",
    "webpack-hot-middleware": "^2.22.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.0.4",
    "mocha-lcov-reporter": "^1.3.0",
    "moxios": "^0.4.0",
    "nightwatch": "^0.9.21",
    "nodemon": "^1.17.1",
    "nyc": "^11.4.1"
  }
}
