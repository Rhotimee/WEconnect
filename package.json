{
  "name": "weconnect",
  "version": "1.0.0",
  "description": "WeConnect provides a platform that brings businesses and individuals together.",
  "main": "./server/server.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev-start": "nodemon --exec babel-node server/server.js",
    "build": "babel server -d ./dist --ignore server/dist",
    "unmigrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all",
    "seed": "node ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "migrate": "npm run unmigrate && node ./node_modules/sequelize-cli/lib/sequelize db:migrate && npm run seed",
    "heroku-postbuild": "npm run migrate",
    "postinstall": "npm run build",
    "test": "npm run migrate && nyc --reporter=html --reporter=text mocha server/tests/ --exit --compilers js:babel-register",
    "test-watch": "nodemon --exec \"npm test\"",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "webpack-dev": "webpack --mode development",
    "webpack-prod": "webpack --mode production",
    "webpack-watch": "webpack --mode development --watch",
    "webpack-start": "webpack-dev-server --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rhotimee/weconnect.git"
  },
  "author": "Yemitan Isaiah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rhotimee/weconnect/issues"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^1.0.3",
    "body-parser": "~1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "~4.15.5",
    "jsonwebtoken": "^8.2.0",
    "morgan": "~1.9.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "sequelize": "^4.36.1",
    "sequelize-cli": "^4.0.0",
    "swagger-ui-express": "^2.0.15",
    "validator": "^9.4.1",
    "webpack": "^4.5.0",
    "webpack-dev-middleware": "^3.1.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "mocha": "^5.0.4",
    "mocha-lcov-reporter": "^1.3.0",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.1",
    "nyc": "^11.4.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  }
}
